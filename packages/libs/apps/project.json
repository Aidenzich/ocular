{
  "name": "apps",
  "$schema": "../../../node_modules/nx/schemas/project-schema.json",
  "sourceRoot": "packages/libs/apps/src",
  "projectType": "library",
  "targets": {
    "build": {
      "executor": "@nx/js:tsc",
      "outputs": ["{options.outputPath}"],
      "options": {
        "outputPath": "dist/packages/libs/apps",
        "main": "packages/libs/apps/src/index.ts",
        "tsConfig": "packages/libs/apps/tsconfig.lib.json",
        "assets": [
          {
            "input": "./packages/libs/apps/src",
            "glob": "**/!(*.ts)",
            "output": "./src"
          },
          {
            "input": "./packages/libs/apps/src",
            "glob": "**/*.d.ts",
            "output": "./src"
          },
          {
            "input": "./packages/libs/apps",
            "glob": "generators.json",
            "output": "."
          },
          {
            "input": "./packages/libs/apps",
            "glob": "executors.json",
            "output": "."
          }
        ]
      }
    },
    "lint": {
      "executor": "@nx/eslint:lint",
      "outputs": ["{options.outputFile}"]
    }
  },
  "tags": []
}
