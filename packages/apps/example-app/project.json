{
  "name": "example-app",
  "$schema": "../../../node_modules/nx/schemas/project-schema.json",
  "sourceRoot": "packages/apps/example-app/src",
  "projectType": "application",
  "targets": {
    "build": {
      "executor": "@nx/webpack:webpack",
      "outputs": ["{options.outputPath}"],
      "defaultConfiguration": "production",
      "options": {
        "compiler": "babel",
        "outputPath": "dist/packages/apps/example-app",
        "index": "packages/apps/example-app/src/index.html",
        "baseHref": "/",
        "main": "packages/apps/example-app/src/main.ts",
        "tsConfig": "packages/apps/example-app/tsconfig.app.json",
        "assets": [
          "packages/apps/example-app/src/favicon.ico",
          "packages/apps/example-app/src/assets"
        ],
        "styles": [],
        "scripts": [],
        "webpackConfig": "packages/apps/example-app/webpack.config.ts"
      },
      "configurations": {
        "development": {
          "extractLicenses": false,
          "optimization": false,
          "sourceMap": true,
          "vendorChunk": true
        },
        "production": {
          "fileReplacements": [
            {
              "replace": "packages/apps/example-app/src/environments/environment.ts",
              "with": "packages/apps/example-app/src/environments/environment.prod.ts"
            }
          ],
          "optimization": true,
          "outputHashing": "all",
          "sourceMap": false,
          "namedChunks": false,
          "extractLicenses": true,
          "vendorChunk": false,
          "webpackConfig": "packages/apps/example-app/webpack.config.prod.ts"
        }
      }
    },
    "serve": {
      "executor": "@nx/react:module-federation-dev-server",
      "defaultConfiguration": "development",
      "options": {
        "buildTarget": "example-app:build",
        "hmr": true,
        "port": 4201
      },
      "configurations": {
        "development": {
          "buildTarget": "example-app:build:development"
        },
        "production": {
          "buildTarget": "example-app:build:production",
          "hmr": false
        }
      }
    },
    "lint": {
      "executor": "@nx/eslint:lint",
      "outputs": ["{options.outputFile}"]
    },
    "serve-static": {
      "executor": "@nx/web:file-server",
      "defaultConfiguration": "production",
      "options": {
        "buildTarget": "example-app:build",
        "watch": false,
        "port": 4201
      },
      "configurations": {
        "development": {
          "buildTarget": "example-app:build:development"
        },
        "production": {
          "buildTarget": "example-app:build:production"
        }
      }
    },
    "test": {
      "executor": "@nx/jest:jest",
      "outputs": ["{workspaceRoot}/coverage/{projectRoot}"],
      "options": {
        "jestConfig": "packages/apps/example-app/jest.config.ts"
      }
    },
    "publish": {
      "executor": "@autoflow-ai/apps:publish"
    },
    "pack":{
      "executor": "@autoflow-ai/apps:pack"
    }
  },
  "tags": []
}
