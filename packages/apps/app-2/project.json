{
  "name": "app-2",
  "$schema": "../../../node_modules/nx/schemas/project-schema.json",
  "sourceRoot": "packages/apps/app-2/src",
  "projectType": "application",
  "targets": {
    "build": {
      "executor": "@nx/webpack:webpack",
      "outputs": ["{options.outputPath}"],
      "defaultConfiguration": "production",
      "options": {
        "compiler": "babel",
        "outputPath": "dist/packages/apps/app-2",
        "index": "packages/apps/app-2/src/index.html",
        "baseHref": "/",
        "main": "packages/apps/app-2/src/main.ts",
        "tsConfig": "packages/apps/app-2/tsconfig.app.json",
        "assets": [
          "packages/apps/app-2/src/favicon.ico",
          "packages/apps/app-2/src/assets"
        ],
        "styles": [],
        "scripts": [],
        "webpackConfig": "packages/apps/app-2/webpack.config.ts"
      },
      "configurations": {
        "development": {
          "extractLicenses": false,
          "optimization": false,
          "sourceMap": true,
          "vendorChunk": true
        },
        "production": {
          "fileReplacements": [
            {
              "replace": "packages/apps/app-2/src/environments/environment.ts",
              "with": "packages/apps/app-2/src/environments/environment.prod.ts"
            }
          ],
          "optimization": true,
          "outputHashing": "all",
          "sourceMap": false,
          "namedChunks": false,
          "extractLicenses": true,
          "vendorChunk": false,
          "webpackConfig": "packages/apps/app-2/webpack.config.prod.ts"
        }
      }
    },
    "serve": {
      "executor": "@nx/react:module-federation-dev-server",
      "defaultConfiguration": "development",
      "options": {
        "buildTarget": "app-2:build",
        "hmr": true,
        "port": 4202
      },
      "configurations": {
        "development": {
          "buildTarget": "app-2:build:development"
        },
        "production": {
          "buildTarget": "app-2:build:production",
          "hmr": false
        }
      }
    },
    "lint": {
      "executor": "@nx/eslint:lint",
      "outputs": ["{options.outputFile}"]
    },
    "serve-static": {
      "executor": "@nx/web:file-server",
      "defaultConfiguration": "production",
      "options": {
        "buildTarget": "app-2:build",
        "watch": false,
        "port": 4202
      },
      "configurations": {
        "development": {
          "buildTarget": "app-2:build:development"
        },
        "production": {
          "buildTarget": "app-2:build:production"
        }
      }
    }
  },
  "tags": []
}
